{
  "name": "nodewarz-server",
  "version": "1.0.0",
  "description": "Multiplayer game server for nodewarz",
  "main": "index.js",
  "scripts": {
    "test": "jest --watch --onlyChanged",
    "test:watch": "npm run test -- --watch",
    "start": "nodemon index.js --exec babel-node --presets env"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/corourke/nodewarz-server.git"
  },
  "keywords": [
    "node",
    "socketio",
    "redux",
    "immutable"
  ],
  "author": "Cameron O'Rourke",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/corourke/nodewarz-server/issues"
  },
  "homepage": "https://github.com/corourke/nodewarz-server#readme",
  "dependencies": {
    "immutable": "^3.8.1",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-immutable": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "socket.io": "^2.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-env": "^2.4.1",
    "babel-preset-es2015": "^6.24.1",
    "jest": "^20.0.4",
    "nodemon": "^1.11.0",
    "pretty-format": "^20.0.3"
  }
